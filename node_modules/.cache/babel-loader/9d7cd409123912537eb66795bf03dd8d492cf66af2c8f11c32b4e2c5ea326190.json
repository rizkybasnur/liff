\nimport ListComponent from \"@/components/ListComponent.vue\";\n\nexport default {\n  components: { FormComponent, ListComponent },\n\n  data() {\n    return {\n      data: [],\n      isDetail: false,\n      dataDetail: null,\n      page: 1,\n      pageSize: 8,\n    };\n  },\n  mounted() {\n    this.getData();\n  },\n  methods: {\n    getData() {\n      const payload = {\n        page: this.page,\n        pageSize: this.pageSize,\n      };\n      api\n        .post(\"/getAllData\", payload)\n        .then((response) => {\n          this.data = response.data;\n        })\n        .catch((error) => {\n          console.error(\"Error fetching data:\", error);\n        });\n    },\n    toggleDetail(e) {\n      this.isDetail = true;\n      this.dataDetail = e;\n    },\n    onInput(e) {\n      this.page = e;\n      this.getData();\n    },\n  },\n};\n</script>\n"],"mappings":"AA6BA,OAAAA,GAAA;AACA,OAAAC,aAAA;AACA,OAAAC,aAAA;AAEA;EACAC,UAAA;IAAAF,aAAA;IAAAC;EAAA;EAEAE,KAAA;IACA;MACAA,IAAA;MACAC,QAAA;MACAC,UAAA;MACAC,IAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACAD,QAAA;MACA,MAAAE,OAAA;QACAL,IAAA,OAAAA,IAAA;QACAC,QAAA,OAAAA;MACA;MACAR,GAAA,CACAa,IAAA,gBAAAD,OAAA,EACAE,IAAA,CAAAC,QAAA;QACA,KAAAX,IAAA,GAAAW,QAAA,CAAAX,IAAA;MACA,GACAY,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,yBAAAA,KAAA;MACA;IACA;IACAE,aAAAC,CAAA;MACA,KAAAf,QAAA;MACA,KAAAC,UAAA,GAAAc,CAAA;IACA;IACAC,QAAAD,CAAA;MACA,KAAAb,IAAA,GAAAa,CAAA;MACA,KAAAV,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}